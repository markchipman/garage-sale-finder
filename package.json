    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-loader": "^1.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.10.0",
    "gh-pages": "^0.12.0",
    "html-webpack-plugin": "^2.17.0",
    "jquery": "^3.1.1",
    "json-loader": "^0.5.4",
    "mapbox": "^1.0.0-beta5",
    "mapbox-gl": "^0.32.1",
    "node-sass": "^4.1.1",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "transform-loader": "^0.2.3",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webworkify-webpack": "^2.0.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.0"
  }
}
