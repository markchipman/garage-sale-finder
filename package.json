{
  "name": "garage-sale-finder",
  "version": "1.0.0",
  "description": "A simple web app used to locate garage sales near you.",
  "scripts": {
    "build": "webpack --config webpack-prod.config.js --colors",
    "dev": "webpack-dev-server --config webpack-dev.config.js --watch --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caseklim/garage-sale-finder.git"
  },
  "keywords": [
    "webpack",
    "frontend",
    "es6",
    "javascript",
    "webdev"
  ],
  "author": "Casey Klimkowsky",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caseklim/garage-sale-finder/issues"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "semi": 2
    }
  },
  "homepage": "https://github.com/caseklim/garage-sale-finder#readme",
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.2.10",
